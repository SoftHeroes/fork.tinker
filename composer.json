{
    "name": "botman/tinker",
    "license": "MIT",
    "description": "BotMan tinker command for your Laravel BotMan project",
    "keywords": [
        "Bot",
        "BotMan",
        "Laravel",
        "Tinker"
    ],
    "homepage": "http://github.com/botman/tinker",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3.0",
        "botman/botman": "dev-master",
        "clue/stdio-react": "^2.3",
        "illuminate/support": "^8.6",
        "illuminate/console": "^8.6"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0 || ^3.5 || ^3.6 || ^3.7 || ^3.8 || ^4.0 || ^5.2 || ^6.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Zeald/fork.botman"
        }
    ],
    "autoload": {
        "psr-4": {
            "BotMan\\Tinker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BotMan\\Tinker\\TinkerServiceProvider"
            ]
        }
    }
}
